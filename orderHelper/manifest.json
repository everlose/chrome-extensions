{
  "name": "点餐助手",
  "version": "1.0",
  "description": "每天的上午9点到下午14点自动触发订餐动作",
  "homepage_url": "https://github.com/everlose/chrome-extentions",
  "permissions": [
    "https://meican.com/*",
    "notifications",
    "tabs"
  ],
  "browser_action": {
    "default_popup": "./html/popup.html",
    "default_title": "点餐助手"
  },
  "icons": {
    "128": "./pic/128.png",
    "16": "./pic/16.png",
    "48": "./pic/48.png"
  },
  "background": {
    "scripts": ["./js/background.js"],
    "persistent": false
  },
  "manifest_version": 2
}
