{
  "name": "FukingBaiduAds",
  "version": "0.0.1",
  "manifest_version": 2,
  "author": "everlose",
  "description": "Remove ads from baidu search result pages",
  "homepage_url": "https://github.com/everlose/chrome-extentions",
  "permissions": ["http://www.baidu.com/*", "https://www.baidu.com/*"],
  "content_scripts": [
    {
      "matches": ["http://www.baidu.com/*", "https://www.baidu.com/*"],
      "js": ["remove.js"]
    }
  ]
}
